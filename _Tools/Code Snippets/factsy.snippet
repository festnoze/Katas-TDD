<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>XUnit fact test sync</Title>
			<Shortcut>factsy</Shortcut>
			<Description>Extrait de code pour créé une méthode de test XUnit Fact synchrone</Description>
			<Author>Etienne MILLERIOUX</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>testname</ID>
					<ToolTip>Nom de la méthode de test</ToolTip>
					<Default>Test</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[[Fact]
	public void $testname$()
	{
    /// Arrange
    
    /// Act
    
    /// Assert
    
}
	$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
